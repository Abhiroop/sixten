Bool : Type
Bool = {n : Size} -> {a : Type {n}} -> a -> a -> a

True : Bool
True x y = x

False : Bool
False x y = y

if : {a : Type} -> Bool -> a -> a -> a
if b = b

and : Bool -> Bool -> Bool
and b1 b2 = if b1 b2 False

or : Bool -> Bool -> Bool
or b1 b2 = if b1 True b2

main1 = printSize (if (or False (and True False)) 12 13)
