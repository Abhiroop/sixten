Bool : Type {1}
Bool = {a : Type {1}} -> a -> a -> a

True : Bool
True x y = x

False : Bool
False x y = y

if : forall {a}. Bool -> a -> a -> a
if b = b

and : Bool -> Bool -> Bool
and b1 b2 = if b1 b2 False

or : Bool -> Bool -> Bool
or b1 b2 = if b1 True b2

main2 = printSize (if (or (and True False) True) 12 13)
