the : (A : Type) -> A -> A
the A a = a

type Bool2 where
  True : Bool2
  False : Bool2

tt : Bool2
tt = True
nil = Nil
list : boolList
list = Cons (the Bool False) (Ref (Cons (the Bool True) (Ref Nil)))
boolList = List Bool

test (b : Bool) = case b of
  False -> 0
  True -> 1

type List a where
  Nil : List a
  Cons : a -> Ptr (List a) -> List a

type Maybe a where
  Nothing : Maybe a
  Just : a -> Maybe a

tail : forall {a}. List a -> List a
tail xs = case xs of
  Nil -> Nil
  Cons x xs' -> deref xs'

tail' xs = case xs of
  Nil -> Nil
  Cons x xs' -> deref xs'

deref : forall {t}. Ptr t -> t
deref p = case p of Ref t -> t

map : forall {a b : Type}. (a -> b) -> List a -> List b
map f xs = case xs of
  Nil -> Nil
  Cons x xs' -> Cons (f x) (Ref (map f (deref xs')))

mapp f xs = case xs of
  Nil -> Nil
  Cons x xs' -> Cons (f x) (Ref (mapp f (deref xs')))

id x = x

propagationTest : Bool
propagationTest = id (id True)

lift f x = f x (\y. y)

main1 = printInt (test propagationTest)
